<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal">





        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/boq_label"
                android:layout_width="150dp"
                android:layout_height="100dp"
                android:fontFamily="@font/lato_medium"
                android:gravity="center"
                android:includeFontPadding="false"
                android:text="Reference No"
                android:background="@drawable/cell_background"
                android:textAlignment="center"
                android:textColor="@color/colorBlack" />


            <TextView
                android:id="@+id/boq_title"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:fontFamily="@font/lato_medium"
                android:gravity="center"
                android:text="Registration Id"
                android:textAlignment="center"
                android:background="@drawable/cell_background"
                android:textColor="@color/colorBlack" />

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="match_parent"
                    android:fontFamily="@font/lato_medium"
                    android:gravity="center"
                    android:text="Property No"
                    android:textAlignment="center"
                    android:background="@drawable/cell_background"
                    android:textColor="@color/colorBlack" />



                    <TextView
                        android:layout_width="500dp"
                        android:layout_height="match_parent"
                        android:fontFamily="@font/lato_medium"
                        android:gravity="center"
                        android:text="Applicant Name"
                        android:textAlignment="center"
                        android:background="@drawable/cell_background"
                        android:textColor="@color/colorBlack" />

                    <TextView
                        android:layout_width="150dp"
                        android:layout_height="match_parent"
                        android:fontFamily="@font/lato_medium"
                        android:gravity="center"
                        android:text="Request Date"
                        android:background="@drawable/cell_background"
                        android:textColor="@color/colorBlack" />

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="match_parent"
                    android:fontFamily="@font/lato_medium"
                    android:gravity="center"
                    android:text="Status"
                    android:textAlignment="center"
                    android:background="@drawable/cell_background"
                    android:textColor="@color/colorBlack" />

                    <TextView
                        android:id="@+id/boq_rate"
                        android:layout_width="250dp"
                        android:layout_height="match_parent"
                        android:fontFamily="@font/lato_medium"
                        android:includeFontPadding="false"
                        android:text="Forwarded To"
                        android:gravity="center"
                        android:background="@drawable/cell_background"
                        android:textColor="@color/colorBlack" />


                    <TextView
                        android:layout_width="150dp"
                        android:layout_height="match_parent"
                        android:background="@drawable/cell_background"
                        android:fontFamily="@font/lato_medium"
                        android:gravity="center"
                        android:includeFontPadding="false"
                        android:text="Forwarded Date "
                        android:textColor="@color/colorBlack" />


                    <TextView
                        android:layout_width="150dp"
                        android:layout_height="match_parent"
                        android:fontFamily="@font/lato_medium"
                        android:includeFontPadding="false"
                        android:text="Approval Date"
                        android:gravity="center"
                        android:background="@drawable/cell_background"
                        android:textColor="@color/colorBlack" />



        </LinearLayout>





</LinearLayout>